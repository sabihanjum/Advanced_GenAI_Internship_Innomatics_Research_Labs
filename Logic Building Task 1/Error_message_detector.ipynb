      "outputs": [],
      "source": [
        "#System logs\n",
        "logs = [\"INFO\", \"ERROR\", \"WARNING\", \"ERROR\"]"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#Count error occurrences\n",
        "error_count = logs.count(\"ERROR\")"
      ],
      "metadata": {
        "id": "K8A92M1N9R0H"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#print result\n",
        "print(\"Total Logs:\", len(logs))\n",
        "print(\"Total ERROR Entries:\", error_count)"
      ],
